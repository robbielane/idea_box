<%= flash[:error] %>
<%= form_for @idea do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :body %>
  <%= f.text_area :body %>
  <%= f.label :category_id, "Catgegory" %>
  <%= f.collection_select :category_id, Category.all, :id, :name %>
  <%= f.submit "Submit" %>
<% end %>
